   //goes where the tree is
	public void insert(int num) {
		if (num < data) {
			if (left != null) {
				left.addNode(num);
			} 
			else {
				left = new Node(num);
			}

		} 
		else {
			if (right != null) {
				right.addNode(num);
			} 
			else {
				right = new Node(num);
			}

		}
	}//end of insert
}
